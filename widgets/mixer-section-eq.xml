<!DOCTYPE template>
<!--
Description:
  A mixer EQ+Filter section.
Variables:
  section-group: The group to connect to.
-->
<Template>
  <WidgetGroup>
    <ObjectName>MixerFilterPart</ObjectName>
    <Layout>vertical</Layout>
    <Size>f,f</Size>
    <Children>
      <WidgetGroup>
        <ObjectName>ContainerHigh</ObjectName>
        <Layout>vertical</Layout>
        <Size>f,f</Size>
        <Children>
          <Template src="skin:widgets/knob.xml">
            <SetVariable name="button_group">QuickEffectRack1_[<Variable name="group"/>]_Effect1</SetVariable>
            <SetVariable name="group">QuickEffectRack1_[<Variable name="group"/>]</SetVariable>
            <SetVariable name="control">super1</SetVariable>
            <SetVariable name="button_control">enabled</SetVariable>
            <SetVariable name="label">Filter</SetVariable>
            <SetVariable name="color">Yellow</SetVariable>
          </Template>
        </Children>
      </WidgetGroup>
      <WidgetGroup>
        <ObjectName>ContainerTopEQ</ObjectName>
        <Layout>vertical</Layout>
        <Size>f,f</Size>
        <Children>
          <Template src="skin:widgets/knob.xml">
            <SetVariable name="button_group">EqualizerRack1_[<Variable name="group"/>]_Effect1</SetVariable>
            <SetVariable name="group">EqualizerRack1_[<Variable name="group"/>]_Effect1</SetVariable>
            <SetVariable name="control">parameter3</SetVariable>
            <SetVariable name="button_control">button_parameter3</SetVariable>
            <SetVariable name="label">High</SetVariable>
            <SetVariable name="color">White</SetVariable>
          </Template>
          <Template src="skin:widgets/knob.xml">
            <SetVariable name="button_group">EqualizerRack1_[<Variable name="group"/>]_Effect1</SetVariable>
            <SetVariable name="group">EqualizerRack1_[<Variable name="group"/>]_Effect1</SetVariable>
            <SetVariable name="control">parameter2</SetVariable>
            <SetVariable name="button_control">button_parameter2</SetVariable>
            <SetVariable name="label">Mid</SetVariable>
            <SetVariable name="color">White</SetVariable>
          </Template>
          <Template src="skin:widgets/knob.xml">
            <SetVariable name="button_group">EqualizerRack1_[<Variable name="group"/>]_Effect1</SetVariable>
            <SetVariable name="group">EqualizerRack1_[<Variable name="group"/>]_Effect1</SetVariable>
            <SetVariable name="control">parameter1</SetVariable>
            <SetVariable name="button_control">button_parameter1</SetVariable>
            <SetVariable name="label">Low</SetVariable>
            <SetVariable name="color">White</SetVariable>
          </Template>
        </Children>
      </WidgetGroup>
    </Children>
  </WidgetGroup>
</Template>